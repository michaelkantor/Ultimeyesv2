<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.logs_ultimeyesvision_com_devdb.data.AvailableLicenseKeys" table="available_license_keys" schema="public" dynamic-insert="false" dynamic-update="false">
        <composite-id name="id" class="com.logs_ultimeyesvision_com_devdb.data.AvailableLicenseKeysId">
            <key-property name="key" type="string">
                <column name="key" length="50"/>
            </key-property>
            <key-property name="createdAt" type="timestamp">
                <column name="created_at" length="29"/>
            </key-property>
            <key-property name="updatedAt" type="timestamp">
                <column name="updated_at" length="29"/>
            </key-property>
        </composite-id>
    </class>
</hibernate-mapping>